<connectorDefinition label="${com.esri.geoevent.adapter.aws-adapter.CONNECTOR_OUT_LABEL}"
  name="send-json-to-aws-iot-gateway" type="outbound">
  <adapter uri="${adapter.outbound.domain}/Generic-JSON/${project.version}"/>
  <transport uri="${transport.outbound.domain}/{component.name}/${project.version}" />
  <defaultName>json-aws-out</defaultName>
  <description>${com.esri.geoevent.adapter.aws-adapter.CONNECTOR_OUT_DESC}</description>
  <properties>
    <shown>
      <property label="Client Endpoint" name="endpoint" source="transport" default="A1OAECNIGB9KXE.iot.us-west-2.amazonaws.com"/>
      <property label="IoT Service Type" name="iotservicetype" source="transport" default="IOT_TOPIC"/>
      <property label="Device Id Field Name" name="deviceid" source="transport" default="MyLightBlub"/>
      <property label="AWS IoT Topic" name="topic" source="transport" default="SBC/test"/>
      <property label="Certificate File" name="X509certificate" source="transport" default="C:/tmp/certs/cert.pem"/>
      <property label="Private Key File" name="privateKey" source="transport" default="C:/tmp/certs/private.pem"/>
    </shown>
    <hidden>
      <property label="MIME Type" name="mimeType" source="adapter" default="application/json"/>
      <property label="Formatted JSON" name="prettyJson" source="adapter" default="false"/>
    </hidden>
  </properties>
</connectorDefinition>